MODULE MachineA()
	VAR
		status : {idle, authing, messaging};
		nonce : boolean;
		authd : boolean;
	ASSIGN
		init(status) := idle;
		init(nonce) := false;
		init(authd) := false;
		next(status) :=
			case
				nonce = false : idle;
				nonce = true : authing;
				esac; 
